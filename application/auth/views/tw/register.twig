{% import 'web/macro/formmacro.twig' as m %}
<form id="{{ form.id }}" method="post" action="{{ form.action }}" accept-charset="UTF-8" enctype="multipart/form-data">
    {{ csrf()|raw }}
    {{ m.input(name) }}
    {{ m.input(email) }}
    {{ m.input(password) }}
    {{ m.input(confirm) }}
    <div class="mt-2">
        {{ m.checkbox(agree) }}
    </div>
    <div class="form-control mt-2">
        <button type="submit" class="btn btn-primary">Sign Up</button>
    </div>
</form>
