{% import 'web/macro/formmacro.twig' as m %}
<form id="{{ _context.form.id }}" method="post" action="{{ _context.form.action }}" accept-charset="UTF-8" enctype="multipart/form-data">
    {{ csrf()|raw }}
    {{ m.input(_context.email) }}
    <div class="form-control mt-4">
        <button type="submit" class="btn btn-primary">Submit</button>
    </div>
</form>
