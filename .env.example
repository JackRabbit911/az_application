MAIL_SMTP_PORT=1025
MAIL_SMTP_AUTH=false
MAIL_PASSWORD=''
MAIL_FROM_ADDRESS=robot@site.zone
MAIL_FROM_NAME="${APP_NAME}"

FTP_HOST=ftp.hostname
FTP_USER=username
FTP_PASSWORD=password

IDE_SEARCH=/var/www/site.zone
IDE_REPLACE=vscode://file/home/alx/www/az_app/site.zone

GIT_REPO="'https://github.com/Owner/projectName'"
