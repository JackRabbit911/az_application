APP_NAME=ProjectName
APP_HOST=localhost
APP_ENV={DEVELOPMENT}
APP_URL=http://${APP_HOST}
APP_TZ=Europe/Moscow

DB_CONNECTION=mysql
DB_HOST=mysql
DB_PORT=3306
DB_DATABASE=test
DB_USERNAME=test
DB_PASSWORD=p@$$word
DB_CHARSET=utf8
DB_ROOT_PASSWORD=secret

SESSION_DRIVER=file
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
